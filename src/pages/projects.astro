---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../config';
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header title={SITE_TITLE} />
		<main>
            <h1>Projects</h1>
            <h2>Current</h2>
            <ul>
                <li><a href="https://apollo.fyi">Apollo - Discord calendar bot</a></li>
                <li><a href="https://github.com/jgayfer/dot">Dot - My personal dotfiles</a></li>
                <li><a href="https://github.com/jgayfer/aureum-tyria">Aureuem Tyria - A toy Rust tool to analyze the GW2 trading post</a></li>
            </ul>
            <h2>Past</h2>
            <ul>
                <li><a href="https://github.com/jgayfer/overwatch_league_bot">Overwatch League Discord Bot</a></li>
                <li><a href="https://github.com/jgayfer/pydest">Pydest - An async Python Destiny 2 API wrapper</a></li>
                <li><a href="https://github.com/jgayfer/spirit">Spirit - A Destiny 2 Discord bot</a></li>
            </ul>
		</main>
		<Footer />
	</body>
</html>
